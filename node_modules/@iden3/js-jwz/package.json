{
  "name": "@iden3/js-jwz",
  "version": "1.11.0",
  "description": "JS implementation of JWZ",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/esm/index.js",
        "require": "./dist/node/cjs/index.cjs"
      },
      "browser": "./dist/browser/esm/index.js",
      "umd": "./dist/browser/umd/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "type": "module",
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:node && npm run build:browser && npm run build:types",
    "build:node": "node ./scripts/node.js",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:esm": "tsc --outDir dist/node/esm  --declaration --declarationDir dist/types",
    "build:browser": "rollup -c scripts/browser.mjs",
    "clean": "rimraf ./dist",
    "test": "vitest run --globals",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "format:check": "prettier --config .prettierrc './{src,tests}/**/*.ts' --check",
    "format": "prettier --config .prettierrc './{src,tests}/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iden3/js-jwz.git"
  },
  "author": "iden3",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/iden3/js-jwz/issues"
  },
  "homepage": "https://github.com/iden3/js-jwz#readme",
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@noble/curves": "^1.9.2",
    "@iden3/js-crypto": "1.3.1",
    "@iden3/js-iden3-core": "1.7.0",
    "@iden3/js-merkletree": "1.5.0",
    "ffjavascript": "0.3.1",
    "rfc4648": "1.5.4",
    "snarkjs": "0.7.5"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.19.4",
    "@iden3/eslint-config": "https://github.com/iden3/eslint-config",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.3",
    "@rollup/plugin-json": "^6.1.0",
    "@types/jest": "30.0.0",
    "@types/node": "^24.0.4",
    "rollup": "^4.44.0",
    "ts-node": "^10.9.2",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
